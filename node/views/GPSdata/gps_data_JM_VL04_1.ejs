<!DOCTYPE html>
<html lang="en">

<head>
  <title>PRODUCT TEST</title>
  <%- include ../logo %>
    <%- include ../layout_datetime_table %>

</head>

<body>
  <%- include ../layout %>
    <div class="main">

      <section class="content">
        <div class="row">
          <div class="col-md-12">
            <div class="box box-primary">
              <div class="box-header with-border" align="center">
                <h3 class="box-title">GPS Logs</h3>

              </div>
              <form class="" action="/loggps_data_JM_VL04/add" method="post">
                <script>

                  console.log("helli");

                </script>
                <div style="display:flex; flex-direction: row; justify-content: left; align-items: center">
                  <label for="imei">หมายเลข IMEI : </label>
                  <input name="imei" type="text" size="14" <%=valueimei %> />
                  <label for="datepicker">กรุณาเลือกวันที่ :</label>
                  <input type="text" id="datepicker" name="cals" <%=valuedate %>>
                  <label for="limit">Limit :</label>
                  <div class="col-sm-1">
                    <select name="limit" id="limit" value="30">
                      <option <%=valuelimit %> ><%=valuelimit2 %>
                      </option>
                      <option value="3">3</option>

                      <option value="10">10</option>

                      <option value="50">50</option>

                      <option value="100">100</option>

                      <option value="500">500</option>


                      <option value="1000">1000</option>
                      <option value="1000">2000</option>

                      <option value="0"> 1 Day</option>



                    </select>

                  </div>
                  <input type="submit" class="btn btn-primary" name="Submit" id="Submit" value="Submit">
                </div>

              </form>
            </div>
            <div class="box">
              <div align="center">imei = --- Port = 11500 </div>
              <div class="box-body">
                <form>
                  <div class="form-group" style=" overflow: auto;height: auto;  ">
                    <div id="data_table_wrapper" class="dataTables_wrapper form-inline dt-bootstrap no-footer">
                      <div class="row">
                        <div class="col-sm-6"></div>
                        <div class="col-sm-6"></div>
                      </div>

                      <div class="row">
                        <div class="col-sm-12">
                          <table id="data_table" class="table table-bordered table-hover dataTable no-footer" border="1"
                            cellpadding="0" cellspacing="0" role="grid">
                            <thead>
                              <%- include ../layout_table_gps2 %>
                                <% if(userData.length!=0){ var i=1; userData.forEach(function(dataX1){ %>
                                  <tr>
                                    <td>
                                      <%=i; %>
                                    </td>
                                    <td>
                                      <%=dataX1.imei %>
                                    </td>
                                    <td>
                                      <%=dataX1.server_stamp_time %>
                                    </td>
                                    <td>
                                      <%=dataX1.gps_stamp_time %>
                                    </td>
                                    <td>
                                      <%=dataX1.latitude %>
                                    </td>
                                    <td>
                                      <%=dataX1.longitude %>
                                    </td>
                                    <td>
                                      <%=dataX1.ignition %>
                                    </td>
                                    <td>
                                      <%=dataX1.gps_speed %>
                                    </td>
                                    <td>
                                      <%=dataX1.gps_odo %>
                                    </td>
                                    <td>
                                      <%=dataX1.gsm_signal %>
                                    </td>
                                    <td>
                                      <%=dataX1.satelites %>
                                    </td>
                                    <td>
                                      <%=dataX1.gps_status %>
                                    </td>
                                    <td>
                                      <%=dataX1.angle %>
                                    </td>
                                    <td>
                                      <%=dataX1.altitude %>
                                    </td>
                                    <td>
                                      <%=dataX1.hdop %>
                                    </td>
                                    <td>
                                      <%=dataX1.pdop %>
                                    </td>
                                    <td>
                                      <%=dataX1.gsm_sim_ccid %>
                                    </td>
                                    <td>
                                      <%=dataX1.ex_battery %>
                                    </td>
                                    <td>
                                      <%=dataX1.in_battery %>
                                    </td>
                                    <td>
                                      <%=dataX1.battery_current %>
                                    </td>
                                    <td>
                                      <%=dataX1.event_id %>
                                    </td>



                                  </tr>
                                  <% i++; }) %>
                                    <% } else{ %>
                                      <tr>
                                        <td colspan="7">No Data Found</td>
                                      </tr>
                                      <% } %>

                            </thead>
                            <tbody>

                            </tbody>
                          </table>
                        </div>
                      </div>

                    </div>
                  </div>

                </form>
              </div>
            </div>
          </div>
        </div>
      </section>

    </div>

</body>

</html>