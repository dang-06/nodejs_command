<!DOCTYPE html>
<html lang="en">

<head>
  <title>PRODUCT TEST</title>
  <%- include ../logo %>
    <title>
      <%= userData %>
    </title>
    <%- include ../layout_datetime_table %>
</head>

<body>
  <%- include ../layout %>
    <div class="main">

      <section class="content">
        <div class="row">
          <div class="col-md-12">
            <div class="box box-primary">
              <div class="box-header with-border" align="center">
                <h3 class="box-title">GPS Logs</h3>

              </div>
              <form class="" action="/logdata_JC400/add" method="post">

                <div style="display:flex; flex-direction: row; justify-content: left; align-items: center">
                  <label for="imei">หมายเลข IMEI2 : </label>
                  <input name="imei" type="text" size="14" <%=valueimei %> />

                  <label for="datepicker">กรุณาเลือกวันที่ :</label>
                  <input type="text" id="datepicker" name="cals" <%=valuedate %>>
                  <label for="limit">Limit : </label>
                  <div class="col-sm-1">
                    <select name="limit">
                      <option <%=valuelimit %> ><%=valuelimit2 %>
                      </option>
                      <option value="3">3</option>

                      <option value="10">10</option>

                      <option value="50">50</option>

                      <option value="100">100</option>

                      <option value="500">500</option>


                      <option value="1000">1000</option>

                      <option value="0"> 1 Day</option>



                    </select>
                  </div>
                  <input type="submit" class="btn btn-primary" name="Submit" id="Submit" value="Submit">
                </div>

              </form>
            </div>
            <div class="box">
              <div align="center">imei = --- Port = 11900 </div>
              <div class="box-body">
                <form>
                  <div class="form-group" style=" overflow: auto;height: 1000px;  ">
                    <div id="data_table_wrapper" class="dataTables_wrapper form-inline dt-bootstrap no-footer">
                      <div class="row">
                        <div class="col-sm-6"></div>
                        <div class="col-sm-6"></div>
                      </div>

                      <div class="row">
                        <div class="col-sm-12">
                          <table id="data_table" class="table table-bordered table-hover dataTable no-footer" border="1"
                            cellpadding="0" cellspacing="0" role="grid">
                            <thead>
                              <%- include ../layout_table_log %>
                                <% if(userData.length!=0){ var i=1; userData.forEach(function(dataX1){ %>
                                  <tr>
                                    <td>
                                      <%=i; %>
                                    </td>
                                    <td>
                                      <%=dataX1.imei %>
                                    </td>
                                    <td>
                                      <%=dataX1.servertime %>
                                    </td>
                                    <td>
                                      <%=dataX1.gpsdata %>
                                    </td>
                                    <td>
                                      <%=dataX1.sever_ack %>
                                    </td>
                                    <td>
                                      <%=dataX1.client_host %>
                                    </td>
                                    <td>
                                      <%=dataX1.client_port %>
                                    </td>
                                  </tr>
                                  <% i++; }) %>
                                    <% } else{ %>
                                      <tr>
                                        <td colspan="7">No Data Found</td>
                                      </tr>
                                      <% } %>

                            </thead>
                            <tbody>

                            </tbody>
                          </table>
                        </div>
                      </div>

                    </div>
                  </div>

                </form>
              </div>
            </div>
          </div>
        </div>
      </section>

    </div>

</body>

</html>